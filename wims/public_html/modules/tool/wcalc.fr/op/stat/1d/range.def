!goto $wims_read_parm

:def
title=Etendue des données
synonyme=etendue statistique, donnees etendue
input=data1d
!exit

:proc

cnt=!itemcnt $formula
fml=
!for i=1 to $cnt
 t=!item $i of $formula
 fml=!append item $[$t] to $fml
!next i
fml=!sort numeric item $fml
min=!item 1 of $fml
max=!item -1 of $fml
result=$[$max-($min)]
!exit

:output

<center>
Etendue = $result
(max = $max, min = $min)
</center> <p>
Données : $cnt nombres { $formula }
!exit

