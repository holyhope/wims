<p>
!if $levelcnt=0
  $name_first3
  !href cmd=reply&job=addlevel $wims_name_addlevel
  !set wims_menu_items=!append line addlevel,1,cmd=reply&job=addlevel\
to $wims_menu_items
.<sup>
  !href cmd=help&special_parm=structure#level $wims_name_help
</sup>
!else
 $table_header
 <caption>$name_first4<sup>
 !href cmd=help&special_parm=structure#level $wims_name_help
 </sup><br><small>
 !href cmd=reply&job=addlevel $wims_name_addlevel
 &nbsp; 
 !href cmd=reply&job=backstruct $wims_name_backstruct
 !set wims_menu_items=!append line addlevel,1,cmd=reply&job=addlevel\
 backstruct,1,cmd=reply&job=backstruct\
to $wims_menu_items
 </small></caption>
 $table_hdtr
 <th>$name_code<th>$wims_name_name</th><th>$wims_name_action</th><th>$name_supervisor2</th>
 <th><small>$name_class</small></th>
 <th><small>$name_program</small></th>
 </tr>
 !for i=1 to $levelcnt
   !set l=!line $i of $leveldata
   !distribute items $l into l_,s_,t_,n_
   !set c=!positionof item $l_ in $classl
   !set c=!itemcnt $c
   !set p=!positionof item $l_ in $progl
   !set p=!itemcnt $p
   $table_tr<td align=center>$l_</td>
   <td align=center>
   !href cmd=reply&job=level&level=$l_ $t_
   </td><td align=center><small>
    !href cmd=reply&job=dellevel&del=$l_ $wims_name_erase
   </small>
   </td><td align=center>
   !if $s_!=$empty
    !if $s_=supervisor
     $wims_name_supervisor
    !else
     !default n_=$s_
     !href cmd=reply&job=modteacher&teacher=$s_ $n_
    !endif
   !else
    $name_nonavailable
   !endif
   </td><td align=center>$c
   </td><td align=center>$p
 !next i
 $table_end
!endif

