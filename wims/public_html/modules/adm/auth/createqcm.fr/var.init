#-------------------------
!read comsDir/formats
!read comsDir/exosDescription

version=v0.6

warning=!record 1 of comsDir/warning
warning=!trim $warning


#------------------------------------------
# redirection vers le répertoire approprié |
#------------------------------------------
!if $exo>3
  demo=oui
  exo=$[$exo-3]
!endif

prefixdir=Dirtyp$(exo)
!read $prefixdir/init



