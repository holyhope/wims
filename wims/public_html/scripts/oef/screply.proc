  
  !if $(diareply$i)=bad and $(precreply$i)!=yes
   !default m_sc_reply$i=0
  !endif
  !if $(precreply$i)=yes and $(diareply$i)=bad
      !default m_sc_reply$i=0.5
  !endif
  !if $(diareply$i)=good
    !if $(partialgood$i)=yes
      !default m_sc_reply$i=0.5
    !else
      !default m_sc_reply$i=1
    !endif
  !endif
