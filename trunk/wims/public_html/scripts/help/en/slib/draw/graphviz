slib_lang_exists_tmp=yes

slib_title=Graphviz
slib_parms=2\
,the code as in <a href="http://www.graphviz.org">Graphviz</a> documentation\
html,format option : <span class="tt">dot</span>, <span class="tt">jpg</span>, <span class="tt">plain</span>, <span class="tt">neato</span>, <span class="tt">html</span>

slib_out=in the case where the format is dot, the url address of the image;\
in the case there is no format, the code to show the image : &lt;img src="..." /> ;\
in the case where the format is jpg, give the coordinates.
