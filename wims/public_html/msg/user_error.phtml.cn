  ``$wims_error_data'' 是被 wims 保留的名称.
  !goto end
!endif

!if no_command iswordof $wims_user_error
  没有命令.
  !goto end
!endif

!if no_insnum iswordof $wims_user_error
 !if $cmd=getfile
