!if $wims_read_parm!=slib_header
!goto proc
!endif

slib_parms=3\
,m\
,a\
,b

slib_author=Sophie, Lemaire; Bernadette, Perrin-Riou

slib_comment=
slib_example= 5,4,6\
[2,3],4,6

slib_require=octave
!exit

:proc
slib_out=
!reset slib_data
!distribute item $wims_read_parm into slib_data,slib_a,slib_b
slib_data=!declosing $slib_data
slib_cnt=!itemcnt $slib_data
!if $slib_cnt=1
	slib_data=1,$slib_data
!endif
slib_c=$[$slib_b^(-1/$slib_a)]
!!deprecated syntax
!!slib_out=!exec octave weibull_rnd($slib_a,$slib_c,[$slib_data])
slib_out=!exec octave wblrnd($slib_c,$slib_a,[$slib_data])

slib_out=!trim $slib_out
slib_out=!replace internal $\
$ by ; in $slib_out
slib_out=!words2items $slib_out
