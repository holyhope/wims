!read scripts/check.user
!if $error!=$empty
 !exit
!endif

data1=!nonempty lines $data1
n=!linecnt $data1
set=0

!readdef $userdeffile
!for i=1 to $n
 l=!line $i of $data1
 l=!translate = to $\
$ in $l
 !distribute lines $l into n_,v_
 !if $n_ isitemof $userdefs and $v_!=$(user_$n_)
  !setdef !set user_$n_=$v_ in $userdeffile
  !advance set
 !endif
!next i

!if $set<1
 error=nothing done
 !exit
!endif

!read adm/class/mkuserlist $qclass

