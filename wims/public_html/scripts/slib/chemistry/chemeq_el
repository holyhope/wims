!if $wims_read_parm!=slib_header
 !goto proc
!endif

slib_author=Georges, Khaznadar
slib_license=GPL

slib_example= Cr2O7^2- + 14H^+ + 6e^- -> 2 Cr^3+ + 7H2O\
Fe^3+ + e^- -> Fe^2+
slib_require=chemeq
!exit

:proc

chemeq_option=e
slib_e=!exec chemeq $wims_read_parm
!reset chemeq_option
slib_out=!replace .*e^-|\([^*]*\)\*.* by \1 in $slib_e

